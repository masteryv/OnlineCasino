<%- include("templates/head.ejs"); %>

<% games.forEach(function(game){ %>
    <div class="game">
        <h3><a href="/games/<%= game.url %>"><%= game.name %></a></h3>
        <img src="/public/img/Default.png">
        <p><%= game.text %></p>
    </div>
<% }); %>

